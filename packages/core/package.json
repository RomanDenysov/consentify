{
  "name": "@consentify/core",
  "version": "0.1.0",
  "description": "Headless cookie consent SDK (zero-deps, TS-first, SSR-safe)",
  "author": {
    "name": "Roman Denysov",
    "url": "https://github.com/RomanDenysov"
  },
  "engines": {
    "node": ">=18.18",
    "pnpm": ">=9"
  },
  "keywords": [
    "cookie",
    "consent",
    "gdpr",
    "ccpa",
    "privacy",
    "typescript",
    "ssr"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RomanDenysov/consentify.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/RomanDenysov/consentify/issues"
  },
  "homepage": "https://github.com/RomanDenysov/consentify#readme",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rimraf": "6.0.1",
    "typescript": "5.9.3"
  }
}